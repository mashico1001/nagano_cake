<div class="container">
  <div class="row">
    <div class="col-10">
      <h4 class="my-3">ジャンル編集</h4>
      <%= form_with model: @genre, url: admin_genre_path(@genre), local:true do |f| %>
        <%= f.label :name, "ジャンル" %>
        <%= f.text_field :name %>
        <%= f.submit '変更を保存', class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>


